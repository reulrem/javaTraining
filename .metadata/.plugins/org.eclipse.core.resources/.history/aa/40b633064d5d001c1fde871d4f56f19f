package P_20211215.game.Ingeritance;

public class Commoner {
	private int lv;
	private String id;
	private int hp;
	private int mp;
	
	public Commoner(String id) {
		lv = 1;
		hp = 20;
		mp = 10;
		this.id = id;
	}
	
	public void getInfo() {

		
		System.out.println("아이디 : " + id + " 레벨: " + lv + " hp : " + hp + " mp : " + mp);	
		
	}
}
