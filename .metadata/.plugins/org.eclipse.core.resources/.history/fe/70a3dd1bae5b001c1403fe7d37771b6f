package garbagecollection_20211213;

public class Main01P201 {
	public static void main(String[] args) {
		Item i1 = new Item(1);
		Item i2 = new Item(2);
		i2 = null;
		Item i3 = new Item(3);
		
		System.gc();
		Item i4 = new Item(1);
		i4 = null;
		Item i5 = new Item(2);
		i2 = null;
		Item i6 = new Item(3);
	}
}
