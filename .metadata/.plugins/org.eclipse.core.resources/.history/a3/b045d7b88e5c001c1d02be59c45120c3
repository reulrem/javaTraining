package P_20211214.Overloading;

public class PartTimer {

	private int pay;

	private boolean noon;

	private String shopName;

	private String name;
	
	public PartTimer(int pay, boolean noon,String shopName,String name) {
		
		this.pay = pay;

		this.noon = noon;

		this.shopName = shopName;

		this.name = name;
		
	}
	
	
}
